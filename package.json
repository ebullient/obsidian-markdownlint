{
  "name": "obsidian-markdownlint",
  "version": "0.0.3",
  "description": "Use markdownlint to make the contents of your files more consistent",
  "main": "main.js",
  "scripts": {
    "prebuild": "npx eslint",
    "build": "node ./esbuild.config.mjs production",
    "postbuild": "cp -v manifest.json README.md styles.css build",
    "dev": "node ./esbuild.config.mjs",
    "preversion": "npm run build",
    "version": "auto-changelog -p",
    "brat-notes": "run() { auto-changelog --stdout --hide-credit --hide-empty-releases --template .github/changelog.hbs -v $1 --starting-version $1  > release-notes.md; }; run"
  },
  "keywords": [
    "obsidian",
    "markdownlint"
  ],
  "author": "ebullient",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lint": "^6.8.2",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.3",
    "markdownlint": "^0.36.1",
    "markdownlint-rule-helpers": "^0.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/eslint__js": "^8.42.3",
    "@types/markdownlint-rule-helpers": "^0.21.5",
    "@types/node": "^22.9.3",
    "auto-changelog": "^2.5.0",
    "builtin-modules": "^4.0.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^9.15.0",
    "obsidian": "^1.7.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.14.0"
  },
  "auto-changelog": {
    "backfillLimit": false,
    "commitLimit": false,
    "ignoreCommitPattern": "(ðŸ”–|ðŸ”¨|ðŸ§¹|changelog|release|Update README).*"
  }
}
